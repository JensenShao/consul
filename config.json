{
	"data_dir": "/consul/data",
	"ui_dir": "/consul/ui",
	"log_level": "INFO",
	"client_addr": "0.0.0.0",
	"ports": {
		"dns": 53
	},
	"recursor": "8.8.8.8",
	"disable_update_check": true,
    "dns_config": {
      "allow_stale": true,
      "max_stale": "2s",
      "node_ttl": "30s",
      "service_ttl": {
        "*": "10s"
      },
    }
}
